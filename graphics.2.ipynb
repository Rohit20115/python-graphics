    "from turtle import*\n",
    "import colorsys\n",
    "title(\"Rohit\")\n",
    "\n",
    "speed(0)\n",
    "bgcolor(\"black\")\n",
    "hue = 0.1\n",
    "pensize(2)\n",
    "goto(300,-60)\n",
    "\n",
    "for i in range(200):\n",
    "    col = colorsys.hsv_to_rgb(hue,1,1)\n",
    "    pencolor(col)\n",
    "    hue+=0.006\n",
    "    lt(150)\n",
    "    circle(400-i,80)\n",
    "    done()"
   ]
