   "source": [
    "from turtle import*\n",
    "import colorsys\n",
    "bgcolor(\"black\")\n",
    "pensize(3)\n",
    "tracer(5)\n",
    "speed(0)\n",
    "setpos(-90,80)\n",
    "\n",
    "h = 0.0\n",
    "for i in range(400):\n",
    "    c = colorsys.hsv_to_rgb(h,1,1)\n",
    "    pencolor(c)\n",
    "    fd(200)\n",
    "    rt(91)\n",
    "    circle(60)\n",
    "    h+=0.009\n",
    "hideturtle()\n",
    "done()"
   ]
