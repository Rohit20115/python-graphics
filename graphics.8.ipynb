    "from turtle import*\n",
    "import colorsys\n",
    "\n",
    "speed(0)\n",
    "bgcolor(\"black\")\n",
    "pensize(2)\n",
    "h=2.8\n",
    "\n",
    "for i in range(100):\n",
    "    c=colorsys.hsv_to_rgb(h,1,1)\n",
    "    pencolor(c)\n",
    "    h+=0.005\n",
    "    circle(-i+1,150)\n",
    "    goto(0,0)\n",
    "    rt(50)\n",
    "    fd(50)\n",
    "done()"
